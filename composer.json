{
    "name": "helsinki-fi/uniarrival",
    "description": "Project for uniarrival.helsinki.fi website.",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^2.3",
        "drupal/config_installer": "~1.7",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-recommended": "^8.9",
        "drupal/mimemail": "^1.0",
        "drupal/warden": "^2.0",
        "drush/drush": "^10.2",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true
    },
        "autoload": {
            "classmap": [
                "scripts/composer/ScriptHandler.php"
            ],
            "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./drupal"
            }
        },
        "installer-paths": {
            "drupal/core": ["type:drupal-core"],
            "drupal/libraries/{$name}": ["type:drupal-library"],
            "drupal/modules/contrib/{$name}": ["type:drupal-module"],
            "drupal/profiles/contrib/{$name}": ["type:drupal-profile"],
            "drupal/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
        }
    }
}
